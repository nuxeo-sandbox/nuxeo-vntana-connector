    "cpr": "^3.0.1",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
